
.m-input-view {
	display: inline-flex;
	flex-direction: row;
	align-items: center;
	/* width: 100%; */
	flex: 1;
	padding: 0 10px;
}
.m-input-input {
	flex: 1;
	width: 100%;
	min-height: 100%;
	line-height: inherit;
	background-color: rgba(0, 0, 0, 0);
}
.m-input-icon {
	width: 20px;
	font-size: 20px;
	line-height: 20px;
	color: #666666;
}

